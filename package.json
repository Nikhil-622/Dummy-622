{
  "name": "viz",
  "version": "2.0.0-alpha.6",
  "description": "Visual regression testing framework",
  "license": "MIT",
  "repository": "steve-taylor/viz",
  "keywords": [
    "visual",
    "regression",
    "puppeteer",
    "react",
    "angular",
    "vue",
    "svelte",
    "jquery",
    "html",
    "css",
    "javascript"
  ],
  "main": "src/server/index.js",
  "bin": "src/server/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "clean": "echo 'CLEAN' && rimraf ./bin/viz.js tmp reports",
    "mkdirs": "echo 'MKDIRS' && mkdirp tmp",
    "build": "npm run -s clean && npm run -s mkdirs && echo 'BUILD' && browserify -t [ babelify --presets [ @babel/preset-env @babel/preset-react @babel/preset-typescript ] ] -t [ envify --NODE_ENV production ] ./src/browser/index.js -o bin/viz.js",
    "watch": "npm run -s clean && npm run -s mkdirs && echo 'WATCH' && watchify -t [ babelify --presets [ @babel/preset-env @babel/preset-react @babel/preset-typescript ] ] -t [ envify --NODE_ENV development ] ./src/browser/index.js -o bin/viz.js",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "core-js": "^3.15.2",
    "envify": "^4.1.0",
    "fs-extra": "^10.0.0",
    "img-diff-js": "^0.5.2",
    "junit-report-builder": "^3.0.0",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "portfinder": "^1.0.28",
    "puppeteer-core": "^10.1.0",
    "recursive-readdir": "^2.2.2",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "serve-handler": "^6.1.3",
    "watchify": "^4.0.0"
  }
}
